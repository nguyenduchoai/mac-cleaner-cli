{
  "name": "mac-cleaner-cli",
  "version": "1.1.8",
  "description": "Open source CLI tool to clean your Mac - remove junk files, caches, logs, and more",
  "type": "module",
  "bin": {
    "mac-cleaner-cli": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsdown src/index.ts --format esm --dts --clean",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "mac",
    "cleaner",
    "macos",
    "cleanup",
    "cache",
    "cli",
    "disk-space",
    "system-maintenance",
    "junk-files"
  ],
  "author": "guhcostan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guhcostan/mac-cleaner-cli.git"
  },
  "bugs": {
    "url": "https://github.com/guhcostan/mac-cleaner-cli/issues"
  },
  "homepage": "https://github.com/guhcostan/mac-cleaner-cli#readme",
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bun": "^1.2.7",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.28.0",
    "tsdown": "^0.17.0",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@inquirer/checkbox": "^5.0.2",
    "@inquirer/confirm": "^6.0.2",
    "chalk": "^5.3.0",
    "commander": "^14.0.2",
    "ora": "^9.0.0"
  },
  "os": [
    "darwin"
  ],
  "engines": {
    "node": ">=20.12.0"
  }
}
